roll(false))\n                      dispatch(deleteCart(index));\n                    }}\n                  >\n                    <div className=\"w-4 h-4\">\n                      <img src={Trash} alt=\"Trash logo\" />\n                    </div>\n                  </motion.div>\n                </div>\n              </Scroll>\n            </div>\n          </div>\n        );\n      })\n    ) : (\n      <span>Your cart is empty</span>\n    );\n  };\n  return (\n    <div className=\" overflow-y-scroll pb-8 flex flex-col gap-10 overflow-x-hidden h-5/6 hide-scrollbar\">\n      {showItem()}\n      <div ref={newItemRef}></div>\n    </div>\n  );\n};\n\nexport default ListItem;\n","/Users/nguyenhieu/Documents/Data/Golden_Sneaker/src/redux/actions/cartActions.js",[],[],"/Users/nguyenhieu/Documents/Data/Golden_Sneaker/src/components/Animation/Scroll.js",["100"],[],"import { useInView } from 'framer-motion';\nimport React, { useRef } from 'react';\n\nconst Scroll = (props) => {\n    const ref = useRef(null);\n    const isInView = useInView(ref, props.loop == \"all\" ? { all: true }:{once: true});\n    // \"translateY(800px)\"\n    return (\n        <section ref={ref} className={props.style}>\n            <div\n                style={{\n                    transform: isInView ? \"none\" : `${props.scroll}`,\n                    opacity: isInView ? 1 : 0,\n                    transition: \"all 0.9s cubic-bezier(0.17, 0.55, 0.55, 1) 0.5s\"\n                }}\n            >\n                {props.children}\n            </div>\n        </section>\n    );\n};\n\nexport default Scroll;",{"ruleId":"101","severity":1,"message":"102","line":6,"column":9,"nodeType":"103","messageId":"104","endLine":6,"endColumn":18},{"ruleId":"101","severity":1,"message":"105","line":1,"column":10,"nodeType":"103","messageId":"104","endLine":1,"endColumn":21},{"ruleId":"106","severity":1,"message":"107","line":28,"column":13,"nodeType":"108","endLine":28,"endColumn":60},{"ruleId":"106","severity":1,